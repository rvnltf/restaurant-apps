{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-underscore-dangle": 0,
        "import/no-extraneous-dependencies": 0,
        "no-restricted-globals": 0,
        "no-useless-return": 0
    }
}
